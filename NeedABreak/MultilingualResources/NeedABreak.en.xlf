<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="fr-FR" target-language="en" original="NEEDABREAK/PROPERTIES/RESOURCES.RESX" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6915.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="NEEDABREAK/PROPERTIES/RESOURCES.RESX" datatype="resx">
        <trans-unit id="ajuster_le_delai" translate="yes" xml:space="preserve">
          <source>Ajuster le délai</source>
          <target state="translated">Adjust the wait time before lockdown</target>
        </trans-unit>
        <trans-unit id="annuler" translate="yes" xml:space="preserve">
          <source>Annuler</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Cancel</target>
        </trans-unit>
        <trans-unit id="executer_au_demarrage" translate="yes" xml:space="preserve">
          <source>Exécuter au démarrage</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Run at startup</target>
        </trans-unit>
        <trans-unit id="explications" translate="yes" xml:space="preserve">
          <source>Lorsqu'on travaille sur écran, il est impératif de respecter un régime de pauses qui doit être fonction de l'intensité du travail. Choisissez le délai approprié en fonction de l'intensité du travail que vous devez accomplir.</source>
          <target state="translated">When working on a screen, it is important to take frequent screen breaks based on your work hours. Choose the appropriate work duration</target>
        </trans-unit>
        <trans-unit id="fermer" translate="yes" xml:space="preserve">
          <source>Fermer</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Close</target>
        </trans-unit>
        <trans-unit id="il_est_l_heure" translate="yes" xml:space="preserve">
          <source>Il est l'heure de bouger !</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">It's break time!</target>
        </trans-unit>
        <trans-unit id="Imminent_locking" translate="yes" xml:space="preserve">
          <source>Verrouillage imminent</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Imminent lockdown</target>
        </trans-unit>
        <trans-unit id="Less_than_a_minute_before_locking" translate="yes" xml:space="preserve">
          <source>Moins d'une minute avant verrouillage</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Less than a minute to lockdown</target>
        </trans-unit>
        <trans-unit id="minutes" translate="yes" xml:space="preserve">
          <source>minutes</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">mins</target>
        </trans-unit>
        <trans-unit id="minutes_before_locking" translate="yes" xml:space="preserve">
          <source>{0:##} minutes avant verrouillage</source>
          <target state="translated">{0:##} minutes to lockdown</target>
        </trans-unit>
        <trans-unit id="one_minute_before_locking" translate="yes" xml:space="preserve">
          <source>Une minute avant verrouillage</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">One minute to lockdown</target>
        </trans-unit>
        <trans-unit id="quitter" translate="yes" xml:space="preserve">
          <source>Quitter</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Exit</target>
        </trans-unit>
        <trans-unit id="travail_habituel" translate="yes" xml:space="preserve">
          <source>Travail habituel</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Usual work</target>
        </trans-unit>
        <trans-unit id="travail_intensif" translate="yes" xml:space="preserve">
          <source>Travail intensif</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Intensive work</target>
        </trans-unit>
        <trans-unit id="travail_peu_intensif" translate="yes" xml:space="preserve">
          <source>Travail peu intensif</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Low-intensive work</target>
        </trans-unit>
        <trans-unit id="travail_tres_intensif" translate="yes" xml:space="preserve">
          <source>Travail très intensif</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Very intensive work</target>
        </trans-unit>
        <trans-unit id="notification_avant_verrouillage" translate="yes" xml:space="preserve">
          <source>Notification avant verrouillage</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Notification before locking</target>
        </trans-unit>
        <trans-unit id="verrouillage_imminent_detail" translate="yes" xml:space="preserve">
          <source>Le poste va se verrouiller dans moins d'une minute. Pensez à sauvegardez vos modifications !</source>
          <target state="translated">The screen will be locked in less than a minute. Remember to save your changes!</target>
        </trans-unit>
        <trans-unit id="verrouillage_imminent_title" translate="yes" xml:space="preserve">
          <source>VERROUILLAGE IMMINENT</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">IMMINENT LOCKDOWN</target>
        </trans-unit>
        <trans-unit id="reporter" translate="yes" xml:space="preserve">
          <source>Reporter</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Postpone</target>
        </trans-unit>
        <trans-unit id="about" translate="yes" xml:space="preserve">
          <source>À propos de NEED A BREAK!</source>
          <target state="translated">About NEED A BREAK!</target>
        </trans-unit>
        <trans-unit id="copyright" translate="no" xml:space="preserve">
          <source>NEED A BREAK!  Copyright (C) 2020  Benoît Rocco</source>
          <target state="final">NEED A BREAK!  Copyright (C) 2020  Benoît Rocco</target>
        </trans-unit>
        <trans-unit id="license" translate="yes" xml:space="preserve">
          <source>Vous pouvez consulter la licence complète</source>
          <target state="translated">You can view full license</target>
        </trans-unit>
        <trans-unit id="undefined" translate="yes" xml:space="preserve">
          <source>(non défini)</source>
          <target state="translated">(undefined)</target>
        </trans-unit>
        <trans-unit id="warranty" translate="yes" xml:space="preserve">
          <source>Ce programme est fourni SANS AUCUNE GARANTIE;</source>
          <target state="translated">This program comes with ABSOLUTELY NO WARRANTY;</target>
        </trans-unit>
        <trans-unit id="contact" translate="yes" xml:space="preserve">
          <source>Contact :</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Contact:</target>
        </trans-unit>
        <trans-unit id="here" translate="yes" xml:space="preserve">
          <source>ici</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">here</target>
        </trans-unit>
        <trans-unit id="inrs" translate="no" xml:space="preserve">
          <source>INRS</source>
          <target state="final">INRS</target>
        </trans-unit>
        <trans-unit id="lock_now" translate="yes" xml:space="preserve">
          <source>Verrouiller</source>
          <target state="translated">Lock now</target>
        </trans-unit>
        <trans-unit id="resume" translate="yes" xml:space="preserve">
          <source>Reprendre</source>
          <target state="translated">Resume</target>
        </trans-unit>
        <trans-unit id="suspend" translate="yes" xml:space="preserve">
          <source>Suspendre</source>
          <target state="translated">Suspend</target>
        </trans-unit>
        <trans-unit id="resume_tooltip" translate="yes" xml:space="preserve">
          <source>Reprendre le décompte. Le décompte avant verrouillage reprendra où il s'était arrêté.</source>
          <target state="translated">Resume counting. The countdown before locking will resume where it left off.</target>
        </trans-unit>
        <trans-unit id="suspend_tooltip" translate="yes" xml:space="preserve">
          <source>Suspendre l'application. Le décompte avant verrouillage sera suspendu.</source>
          <target state="translated">Suspend the app. The countdown before locking will be suspended.</target>
        </trans-unit>
        <trans-unit id="resumed_message" translate="yes" xml:space="preserve">
          <source>Décompte avant verrouillage repris</source>
          <target state="translated">Countdown before lockdown resumed</target>
        </trans-unit>
        <trans-unit id="resumed_title" translate="yes" xml:space="preserve">
          <source>NEED A BREAK! réactivé</source>
          <target state="translated">NEED A BREAK! resumed</target>
        </trans-unit>
        <trans-unit id="suspended_message" translate="yes" xml:space="preserve">
          <source>Décompte avant verrouillage suspendu</source>
          <target state="translated">Countdown before lock suspended</target>
        </trans-unit>
        <trans-unit id="suspended_title" translate="yes" xml:space="preserve">
          <source>NEED A BREAK! suspendu</source>
          <target state="translated">NEED A BREAK! suspended</target>
        </trans-unit>
        <trans-unit id="automatic_suspension" translate="yes" xml:space="preserve">
          <source>Suspension automatique</source>
          <target state="translated">Automatic suspension</target>
        </trans-unit>
        <trans-unit id="automatic_suspension_tooltip" translate="yes" xml:space="preserve">
          <source>L'application se suspend automatiquement lorsqu'elle détecte qu'une application plein écran ou une présentation a démarré et se réactive automatiquement lorsque l'application plein écran ou la présentation est terminé.</source>
          <target state="translated">The app automatically suspends when it detects that a full-screen app or presentation has started and automatically reactivates when the full-screen app or presentation is complete.</target>
        </trans-unit>
      </group>
